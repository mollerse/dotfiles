# PATH like a bawss
PATH=$PATH:/usr/local/bin
PATH=$PATH:$HOME/local/bin
PATH=$PATH:$HOME/bin
PATH=$PATH:/sbin
PATH=$PATH:$HOME/.cabal/bin

export PATH

[ -s $HOME/.nvm/nvm.sh ] && . $HOME/.nvm/nvm.sh # This loads NVM
[[ -r $NVM_DIR/bash_completion ]] && . $NVM_DIR/bash_completion #NVM tab completion